{
  "compilerOptions": {
      "declaration": false,
      "module": "commonjs",
      "target": "es6",
      "types": [
        "jasmine",
        "jasmine-ajax",
        "node"
      ],  
      "noImplicitAny": false,
      "sourceMap": false,
      "moduleResolution": "node",
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
          "*": [
              "node_modules/*",
              "src/types/*"
          ]
      }
  },
  "include": [
      "src/**/*",
      "node_modules/@types"
  ]
}